-- last 3 monsters are not listed in clsid so I used direct id
function get_bone_prop(c,bone)
	local creature_t = creatures_cls[c]
	local creature_b = creature_t and creature_t[bone]
	return (creature_t and creature_b) or (creatures_cls[clsid.script_stalker][bone]) or {0, normal_damage}
end

-- code demonized
-- return if hitted bone is critical
function is_critical_bone(c, bone)
	local creature_t = creatures_cls[c]
	if not creature_t then return false end

	local creature_b = creature_t[bone]
	if not creature_b then return false end

	--printf("current bone %s, name %s, critical %s", bone, creature_b[1], creature_b[2] == critical_damage)
	return creature_b[2] == critical_damage
end
-- code demonized end

-- code ilrathCXV
-- return if hit bone is legs
function is_leg_bone(c, bone)
	local creature_t = creatures_cls[c]
	if not creature_t then return false end

	local creature_b = creature_t[bone]
	if not creature_b then return false end
	
	if (creature_t == poltergeist) then 
		return false
	
	elseif creature_t == gigant then
		return creature_b[2] == normal_damage
	
	elseif creature_t == snork then
		if bone >= 50 then return true end
	
	elseif (creature_t == tushkano) or (creature_t == flesh) or (creature_t == controller) or (creature_t == rat) or (creature_t == dog) or (creature_t == boar) then
		if bone >= 0 and bone <= 10 then return true end
	
	elseif (creature_t == zombie) or (creature_t == fracture) or (creature_t == cat) or (creature_t == sucker) then
		if bone >= 0 and bone <= 8 then return true end
	
	elseif (creature_t == karlik) then
		if bone >= 0 and bone <= 11 then return true end
	
	elseif (creature_t == human) then
		if bone >= 2 and bone <= 10 then return true end
	
	elseif (creature_t == burer) then
		if bone >= 9 and bone <= 19 then return true end
	
	elseif (creature_t == chimera) then
		if (bone >= 0 and bone <= 20) then return true end
	
	elseif (creature_t == karlik) or (creature_t == tushkano) or (creature_t == tushkano) or (creature_t == tushkano) or (creature_t == tushkano) or (creature_t == tushkano) then
		if bone >= 0 and bone <= 10 then return true end
	
	else return false
	end

end